#!/usr/bin/env python

import sys
import os

import mxdc.conf

sys.path.append(os.path.dirname(os.path.dirname(__file__)))

from mxdc import conf
conf.initialize()

from mxdc.mxdcapp import MXDCApp
from mxdc.utils.log import log_to_console

if __name__ == "__main__":
    log_to_console()
    app = MXDCApp()
    app.run()
